			Стр = ""В ТЧ <"" +ИмяТЧ  + ""> поле с заголовком <"" + ИмяРеквизита + ""> не найдено!"";
		КонецЕсли;	 
		Сообщить(Стр);
		ВызватьИсключение Стр;
	КонецЕсли;	 
	
	ПолеРеквизит.Активизировать();
	СделатьДействияПриЗаписиВидео();
	СделатьДействияПриЗаписиHTML();
